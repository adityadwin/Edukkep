{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php@0.6.0"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
      "APP_NAME": "Edukkep",
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://nama-proyek-anda.vercel.app",
      "VIEW_COMPILED_PATH": "/tmp/views",
      "SESSION_DRIVER": "array"
    }
}